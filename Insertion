#include<stdio.h>
#define MAX 100
struct node
{
	int data;
	struct node *address; 
}node;
void insertatendLL(struct node*data,intdata,*head)
{
 struct Node* node*(struct Node* )malloc(sizeof(struct node*));
node->data=data;
node->add=NULL;
if(head!=NULL)
{
	head=node;
}
else
{
	struct Node*
    s=head;
while(s->add!=NULL)
{
	s=s->add;
}
s->add=node;
}

